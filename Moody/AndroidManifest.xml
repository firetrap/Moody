<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.moody"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="10"
        android:targetSdkVersion="14" />

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name="com.example.moody.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.moody.Menu_esq"
            android:label="@string/title_activity_menu_esq" >
        </activity>
        <activity
            android:name="com.example.moody.BaseActivity"
            android:label="@string/title_activity_base" >
        </activity>
        <activity
            android:name="com.example.moody.MainView"
            android:label="@string/title_activity_main_view" >
        </activity>
    </application>

</manifest>
